void lcd_send(BYTE a, BYTE cmd)
{
BYTE c;
LATAbits.LATA3 = 0;
LATAbits.LATA2 = 0;

LATAbits.LATA1 = 1;
if(cmd == 0) LATAbits.LATA1 = 0;

asm(" nop");
LATAbits.LATA2 = 1;
for(c=8;c>0;c--)
{
LATAbits.LATA2 = 0;
LATAbits.LATA1 = 0;
if( a &0x80) LATAbits.LATA1 = 1;
asm(" nop");
LATAbits.LATA2 = 1;
a = a<<1;
}
LATAbits.LATA3 = 1;
}
void color_fill(BYTE x1, BYTE x2, BYTE y1, BYTE y2, BYTE color)
{
unsigned long i, total;
total = ( (unsigned long)x2-x1+1)*(y2-y1+1);
lcd_send(0x2A, 0);
lcd_send(x1, 1);
lcd_send(x2, 1);

lcd_send(0x2B, 0);
lcd_send(y1, 1);
lcd_send(y2, 1);

lcd_send(0x2C, 0);
for(i=0; i<total; i++) lcd_send(color, 1);

}
