void lcd_putch(BYTE c, BYTE fColor, BYTE bColor)
{
BYTE col, row;
if (c>'z' || c<' ') {
return;
}
for(col=0; col<5; col++){
for(row=0; row<8; row++){
if( (lcd_font[c-' '][col]) & (0x80>>row) )
lcd_send(fColor, 1);
else
lcd_send(bColor, 1);
}
}
for(row=0; row<8; row++){
lcd_send(bColor, 1);
}
}

void lcd_text(const char *s, BYTE x, BYTE y, BYTE fColor, BYTE bColor)
{
lcd_send(0x36, 0);

lcd_send(0b10100000, 1);

lcd_send(0x2A, 0);
lcd_send(y, 1);
lcd_send(y + 7, 1);

lcd_send(0x2B, 0);
lcd_send(x, 1);
lcd_send(131, 1);

lcd_send(0x2C, 0);

while (*s) {
lcd_putch(*s, fColor, bColor);
++s;
}
}

